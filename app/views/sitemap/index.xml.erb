<%
now = DateTime.now
available_jobs_filter = ["date_available is not null and date_available <= ? and (date_expires is null or date_expires >= ?)", now, now]
%>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">  
  <%= raw url_node('/', '1.0') %>
  <%= raw url_node('/truck-driver-job-application/') %>  
  <%= raw url_node('/policy/') %>
  <%= raw url_node('/s/') %>        
  <%= raw url_node('/companies/') %>
  <%= raw url_node('/companies-hiring/') %>    
  <%= raw url_node('/blog/') %>    
<% Company.where(:status => Company::STATUS_ACTIVE).reorder(:name).all.each do |c| %>  
  <%= raw url_node("/#{c.slug}-jobs/") %>
  <%= raw url_node("/#{c.slug}-job-application/") %>       
  <% c.job_postings.where(available_jobs_filter).all.each do |j| %>        
  <%= raw url_node("/#{c.slug}-job-application/#{j.slug}/") %>
  <%= raw url_node("/job/#{j.slug}/") %>
  <% end %>
  <%= raw url_node("/companies/#{c.slug}/") %>  
<% end %>

<% Category.reorder(:name).all.each do |c| %>
  <%= raw url_node("/#{c.slug}-truck-driver-jobs/") %>
<% end %>

<% States.all.each do |abbr, state| %>
  <%= raw url_node("/truck-driver-jobs-in-#{state.downcase.gsub(' ', '-')}/") %>
<% end %>
