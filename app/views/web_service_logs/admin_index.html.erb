<h1>Web Service Logs</h1>
<p><a href='#' onclick="$('#search_form').slideToggle();">Show/Hide Search Form</a></p>

<%= render :partial => 'web_service_logs/search_form' %>

<% if (@web_service_logs.count > 0) %>
  <table class='data'>
    <tr><%= raw @pager.sortable_table_headings({
      'date_processed'  => 'Date processed',
      'params'          => 'Params',
      'request'         => 'Request'      
    }) %></tr>
    <% @web_service_logs.each do |log| %>
      <tr onclick="window.location='/admin/web-service-logs/<%= log.id %>';">
        <td><%= log.date_processed.strftime('%F %T') %></td>
        <td><%= log.params %></td>
        <td><%= log.request %></td>                
      </tr>
    <% end %>
  </table>
  <p><%= raw @pager.generate %></p>
<% else %>
  <p>No logs found.</p>
<% end %>
